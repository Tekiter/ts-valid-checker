{
  "name": "ts-valid-checker",
  "version": "0.0.1",
  "description": "Readymade validators with type guards",
  "main": "ts-valid-checker.js",
  "repository": "https://github.com/Tekiter/ts-valid-checker.git",
  "tags": [
    "validator",
    "validation",
    "typescript"
  ],
  "sideEffects": false,
  "contributors": [
    "Tekiter <tekiter8@gmail.com>"
  ],
  "license": "MIT",
  "scripts": {
    "build": "webpack build",
    "clean": "rimraf ./dist",
    "test": "jest",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage",
    "lint": "npm run lint-eslint && npm run lint-prettier",
    "lint-eslint": "eslint --ignore-path .gitignore src/**/*{js,ts} --fix",
    "lint-prettier": "prettier --write \"src/**/*.(js|ts)\""
  },
  "lint-staged": {
    "src/**/*.(ts|js)": [
      "yarn lint"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}
